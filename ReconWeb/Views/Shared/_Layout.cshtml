    <script>
        // Show the loader icon
        $(document).ajaxStart(function () {
            // $('#loader').show();
            showProgress();
        });

        // Hide the loader icon
        $(document).ajaxStop(function () {
            // $('#loader').hide();
            hideProgress();
        });


        var spinnerVisible = false;

        function showProgress() {
            if (!spinnerVisible) {
                $("div#spinner").fadeIn("fast");
                spinnerVisible = true;
            }
        }
        function hideProgress() {
            if (spinnerVisible) {
                var spinner = $("div#spinner");
                spinner.stop();
                spinner.fadeOut("fast");
                spinnerVisible = false;
            }
        }

        function Editcustdetails() {

            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                data: {},
                dataType: "html",
                url: '@Url.Action("EmpProfilePartialView", "_Layout")',
                success: function (data) {

                    $("#Newcust1").html(data);
                },
                error: function (data) {
                    $.alert({
                        title: 'Impact',
                        content: 'Error' + data,
                        type: 'red',
                    });
                }
            });
        }

        function kendodate_format() {
            $(".cusDate").kendoDatePicker({
                value: new Date(),
                format: "dd/MM/yyyy"
            });
            $(".cusDate1").kendoDatePicker({
                value: new Date(),
                format: "yyyy-MM-dd"
            });
        }
        function date_validate(event) {
            var x = event.charCode;
            if ((x >= 48 && x <= 57) || (x == 47) || (x == 0) || (x == /^([a-zA-Z]{3,16})$/)) {
                return true;
            }
            else {
                return false;
            }
        }

        function Signout() {


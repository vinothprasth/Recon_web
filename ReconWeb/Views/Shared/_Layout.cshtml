            window.location.href = url;
        }
        function getFormated_StringDate(date) {   //get DD/MM/YYYY or MM/DD/YYYYY
            if (date != "" || date != undefined) {
                var d = date.split("-");
                return d[2] + "-" + d[1] + "-" + d[0];
            }
        }

        window.setTimeout(function () {
            window.location.href = '@Url.Action("session_expires", "Login")'
        }, 1000*25*60);
    </script>

